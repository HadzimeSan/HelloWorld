city: 0.5;\n  cursor: not-allowed;\n}\n<\/style>\n\n<script>\ndocument.querySelectorAll('.accordion-title').forEach(btn => {\n  btn.addEventListener('click', function() {\n    const item = this.parentElement;\n    item.classList.toggle('active');\n  });\n});\n\n\/\/ Disable buy button until size is selected\nfunction updateCartButtonState(select) {\n  const btn = select.closest('.ctl-item').querySelector('.ctl-cart');\n  if (select.selectedIndex === 0) {\n    btn.disabled = true;\n    btn.classList.add('disabled');\n  } else {\n    btn.disabled = false;\n    btn.classList.remove('disabled');\n  }\n}\ndocument.querySelectorAll('.ctl-size').forEach(select => {\n  updateCartButtonState(select);\n  select.addEventListener('change', function() {\n    updateCartButtonState(this);\n  });\n});\n<\/script>"}},"smart_stories_smart_stories_AzYmNW":{"type":"shopify:\/\/apps\/smart-stories\/blocks\/smart-stories\/dfb95db1-52c1-4619-9def-bc221ea020d9","settings":{"story-secret":"","use_default_settings":true,"data_type":"default","dynamic_collection":"","primary_color":"#000000","secondary_color":"#000000","corner_radius":10,"show_group_name":false,"autoplay_audio":false,"font":"helvetica_n4","story_name_display":"0","fill":false,"skew_sync":false,"full_width":false,"floaty_theme":"0","floaty_corner_position":"0","vertical_thumb_spacing":25,"horizontal_thumb_spacing":25,"border_thickness":0,"border_gradient_ratio":50,"border_gradient_degree":50}}},"block_order":["liquid_HBReFP","text_HKnwe4","price","separator","size_chart_button_bAjhqR","payment_terms","promo_banner_rFdkzy","image_XNqh3A","buy_buttons","vimonial_shoppable_videos_ugc_app_block_carousel_product_JRLz3d","product_snapshot_VqFk6Y","liquid_Rb6gwD","liquid_aXdiPK","smart_stories_smart_stories_AzYmNW"],"settings":{"color_scheme":"scheme-2","container_size":"lg","product_info_size":36,"show_sticky_add_to_cart":false,"center_basic_info":false,"desktop_media_layout":"grid_2x","desktop_media_grid_gap":0,"mobile_controls":"d